{
    "name": "digitalcloud/nova-permission-tool",
    "description": "A Laravel Nova Permission tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^4.4",
        "spatie/laravel-permission": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "DigitalCloud\\PermissionTool\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCloud\\PermissionTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
